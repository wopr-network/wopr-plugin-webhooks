{
  "author": "WOPR",
  "description": "HTTP webhook ingress for WOPR - trigger agent runs from external systems",
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/node": "^25.3.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "wopr",
    "plugin",
    "webhooks",
    "automation"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@wopr-network/wopr-plugin-webhooks",
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "1.0.0",
  "wopr": {
    "plugin": {
      "entry": "dist/index.js"
    }
  },
  "dependencies": {
    "@wopr-network/plugin-types": "^0.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
